INSERT INTO `sites` (`SiteID`, `SiteName`, `Location`, `Description`, `SiteImg1`, `SiteImg2`, `Reviews`) VALUES
(1, 'Carolina Site (USA)', 'North Carolina, USA', 'Discover the wonders of Carolina, USA, where nature\'s beauty and adventure await. Our campsite is nestled amidst the majestic Blue Ridge Mountains, offering breathtaking views and a serene escape from the everyday.', 'fileImages/_northCarolinaSite1.jpg', 'fileImages/_northCarolinaSite2.jpg', '4'),
(2, 'New Forest Glamping Site (UK)', 'New Forest National Park, Hampshire, England', 'Immerse yourself in the enchanting woodlands of New Forest National Park. This glamping site offers luxurious camping experiences with comfortable accommodations.', 'fileImages/_siteNewForestGlamping1.jpg', 'fileImages/_siteNewForestGlamping2.jpg', '4'),
(3, 'Pembrokeshire Seaside Campground (UK)', 'Pembrokeshire, Wales, England', 'Enjoy a coastal retreat at this Pembrokeshire campsite. Explore the rugged coastline, sandy beaches, and ancient castles.', 'fileImages/_sitePembrokeshire1.jpg', 'fileImages/_sitePembrokeshire2.jpg', '4'),
(4, 'Norfolk Broads Nature Campsite (UK)', 'Norfolk Broads, Norfolk, England', 'Embrace the tranquility of the Norfolk Broads, a unique network of rivers and wetlands. This campsite offers a peaceful escape with opportunities for boat trips, and observing wildlife.', 'fileImages/_siteNorfolkBroad1.jpg', 'fileImages/_siteNorfolkBroad2.jpg', '4'),
(5, 'Everglades National Park Campsite (US)', 'Florida, US', 'Experience the vast wilderness of the Everglades, the largest subtropical wilderness in the United States.', 'fileImages/_siteEverglades1.jpg', 'fileImages/_siteEverglades2.jpg', '5'),
(6, 'Nanji Hangang Park', 'South Korea', 'Nanji Hangang Park on the Han River is the largest, with the capacity to accommodate up to 2,500 people in a total of 165 camping pitches.', 'fileImages/_Site-Nanji Hangang Park1.jpeg', 'fileImages/_Site-Nanji Hangang Park2.jpg', '4');

